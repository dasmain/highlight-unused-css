{
  "name": "highlight-unused-css",
  "displayName": "Highlight Unused CSS",
  "description": "Highlight Unused Global CSS Classes From Files or Project",
  "version": "0.0.1",
  "icon": "highlight-css.png",
  "publisher": "dasmain",
  "engines": {
    "vscode": "^1.104.0"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dasmain/highlight-unused-css"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:scss",
    "workspaceContains:src/index.scss",
    "workspaceContains:src/index.css"
  ],
  "contributes": {
    "commands": [],
    "languages": [
      {
        "id": "scss",
        "aliases": [
          "SCSS"
        ],
        "extensions": [
          ".scss"
        ]
      }
    ]
  },
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.104.0",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "fast-glob": "^3.3.3",
    "postcss": "^8.5.6",
    "postcss-scss": "^4.0.9"
  }
}
